              TextFormField(
                onChanged: (value) {
                  setState(() {
                    text = value;
                  });
                },
              ),
              ElevatedButton(
                onPressed: () async {
                  await _pickImage();
                },
                child: Text('Pick Image'),
              ),
              if (isImagePicked)
                imageFile != null
