5.34315 4 7 4H8.53513ZM8 6H7C6.44772 6 6 6.44772 6 7V19C6 19.5523 6.44772 20 7 20H17C17.5523 20 18 19.5523 18 19V7C18 6.44772 17.5523 6 17 6H16C16 7.10457 15.1046 8 14 8H10C8.89543 8 8 7.10457 8 6Z" fill="currentColor"></path></svg>Copy code</button></div><div class="p-4 overflow-y-auto"><code class="!whitespace-pre hljs language-dart">import 'package:flutter/material.dart';
import 'package:audio_recorder/audio_recorder.dart';
import 'dart:async';

class AdminVoiceMessagePage extends StatefulWidget {
  @override
  _AdminVoiceMessagePageState createState() =&gt; _AdminVoiceMessagePageState();
}

class _AdminVoiceMessagePageState extends State&lt;AdminVoiceMessagePage&gt; {
  late bool _isRecording;
  late String _recordingPath;
  late Timer _timer;
  int _start = 0;

  @override
  void initState() {
    super.initState();
    _isRecording = false;
    _recordingPath = '';
  }

  Future&lt;void&gt; _startRecording() async {
    try {
      if (await AudioRecorder.hasPermissions) {
        await AudioRecorder.start();
        _startTimer();
        setState(() {
          _isRecording = true;
          _recordingPath = DateTime.now().toString(); // Placeholder for recording path
        });
      } else {
        print('Permission not granted');
      }
    } catch (e) {
      print('Failed to start recording: $e');
    }
  }

  Future&lt;void&gt; _stopRecording() async {
    if (_isRecording) {
      try {
        await AudioRecorder.stop();
        _stopTimer();
        setState(() {
          _isRecording = false;
        });
      } catch (e) {
        print('Failed to stop recording: $e');
      }
    }
  }

  void _startTimer() {
    const oneSec = const Duration(seconds: 1);
    _timer = new Timer.periodic(
      oneSec,
      (Timer timer) =&gt; setState(() {
        if (_start == 60) {
          _stopRecording();
          timer.cancel();
        } else {
          _start++;
        }
      }),
    );
  }

  void _stopTimer() {
    _timer.cancel();
    _start = 0;
  }

  @override
  void dispose() {
    _stopTimer();
    super.dispose();
  }

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        backgroundColor: Colors.pink,
        title: const Text('Admin Voice Message'),
        centerTitle: true,
      ),
      body: Container(
        color: Colors.pink,
        child: Center(
          child: Padding(
            padding: const EdgeInsets.all(20.0),
            child: Column(
              mainAxisAlignment: MainAxisAlignment.center,
              children: [
                Text(
                  '$_start seconds',
                  style: TextStyle(fontSize: 24),
                ),
                const SizedBox(height: 20),
                if (!_isRecording)
                  ElevatedButton(
                    onPressed: _startRecording,
                    child: Text(
                      'Start Recording',
                      style: TextStyle(fontSize: 18),
                    ),
                  )
                else
                  ElevatedButton(
                    onPressed: _stopRecording,
                    child: Text(
                      'Stop Recording',
                      style: TextStyle(fontSize: 18),
                    ),
                  ),
              ],
            ),
          ),
        ),
      ),
    );
  }
