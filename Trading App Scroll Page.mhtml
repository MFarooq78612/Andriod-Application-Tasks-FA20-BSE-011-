import 'package:permission_handler/permission_handler.dart';
import 'package:audio_recorder/audio_recorder.dart';
import 'package:path_provider/path_provider.dart';

class AdminVoiceMessagePage extends StatefulWidget {
  @override
  _AdminVoiceMessagePageState createState() =&gt; _AdminVoiceMessagePageState();
}

class _AdminVoiceMessagePageState extends State&lt;AdminVoiceMessagePage&gt; {
  late Recording _recording;
  bool _isRecording = false;

  @override
  void initState() {
    super.initState();
    _recording = Recording();
  }

  Future&lt;void&gt; _startRecording() async {
    var status = await Permission.microphone.request();
    if (status != PermissionStatus.granted) return;

    Directory appDocDirectory = await getApplicationDocumentsDirectory();
    String filePath = '${appDocDirectory.path}/recording_${DateTime.now().millisecondsSinceEpoch}.aac';

    try {
      bool hasPermission = await AudioRecorder.hasPermissions;
      if (!hasPermission) return;

      await AudioRecorder.start(
        path: filePath,
        audioOutputFormat: AudioOutputFormat.AAC,
      );
      setState(() {
        _isRecording = true;
      });
    } catch (e) {
      print(e.toString());
    }
  }

  Future&lt;void&gt; _stopRecording() async {
    try {
      await AudioRecorder.stop();
      setState(() {
        _isRecording = false;
      });
    } catch (e) {
      print(e.toString());
    }
  }

  Future&lt;void&gt; _playRecording() async {
    // Implement playing logic here using the recorded file path
  }

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        backgroundColor: Colors.pink,
        title: const Text('Admin Voice Message'),
        centerTitle: true,
      ),
      body: Container(
        color: Colors.black87,
        child: Center(
          child: Column(
            mainAxisAlignment: MainAxisAlignment.center,
            children: [
              if (_isRecording)
                ElevatedButton(
                  onPressed: _stopRecording,
                  child: Text('Stop Recording'),
                )
              else
                ElevatedButton(
                  onPressed: _startRecording,
                  child: Text('Start Recording'),
                ),
              const SizedBox(height: 20),
              ElevatedButton(
                onPressed: _playRecording,
                child: Text('Play Voice'),
              ),
            ],
          ),
        ),
      ),
    );
